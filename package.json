{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build && hash.bat",
    "test": "umi test --runInBand --detectOpenHandles",
    "test:coveralls": "umi test --online | coveralls",
    "add": "node ./config/template.js",
    "preview": "http-server --gzip --port=8080 ./dist -o /config",
    "cover": "http-server --gzip --port=8081 ./coverage/lcov-report -o /",
    "anany": "cross-env ANALYZE=1 umi build && hash.bat",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "precommit": "lint-staged",
    "deploy": "umi build && hash.bat && deploy.bat"
  },
  "gitHooks": {},
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.5.0",
    "@ant-design/pro-layout": "^6.5.0",
    "@draft-js-plugins/editor": "^4.1.0",
    "@handsontable/react": "^8.4.0",
    "@umijs/preset-react": "1.x",
    "antd": "^4.12.3",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.3.1",
    "crypto-js": "^4.0.0",
    "dayjs": "^1.10.4",
    "exceljs": "^4.2.1",
    "file-saver": "^2.0.5",
    "handsontable": "^7.4.2",
    "html2canvas": "^1.0.0-rc.7",
    "jsencrypt": "^3.0.3",
    "jstat": "^1.9.4",
    "jszip": "^3.6.0",
    "linaria": "^2.1.0",
    "lodash-decorators": "^6.0.1",
    "lodash-es": "^4.17.21",
    "raf-schd": "^4.0.3",
    "ramda": "^0.27.1",
    "react-beautiful-dnd": "^13.1.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-highlight-words": "^0.17.0",
    "react-use": "^17.1.1",
    "umi": "^3.3.7",
    "use-memo-one": "^1.1.2",
    "xlsx": "^0.17.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.2",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.19",
    "@types/ramda": "^0.27.29",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-test-renderer": "^16.0.3",
    "@umijs/fabric": "^2.5.6",
    "@umijs/plugin-esbuild": "^1.1.0",
    "@umijs/test": "^3.3.7",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.16.0",
    "eslint-config-umi": "^1.4.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.0.7",
    "msw": "^0.26.2",
    "prettier": "^2.2.1",
    "react": "17.x",
    "react-dom": "17.x",
    "react-test-renderer": "^17.0.1",
    "tslint": "^6.1.3",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^5.0.0",
    "typescript": "^4.1.2",
    "typescript-eslint": "^0.0.1-alpha.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "umi-types": "^0.5.14",
    "yorkie": "^2.0.0"
  }
}
